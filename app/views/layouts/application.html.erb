<!DOCTYPE html>
<html>
<head>
  <title>KiloSoundscape</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>

  <script src="https://connect.soundcloud.com/sdk/sdk-3.1.2.js"></script>
	<link href="https://fonts.googleapis.com/css?family=EB+Garamond|Karla" rel="stylesheet">
  <meta name="viewport" content="width=device-width" />

</head>
<body>
<nav class="navbar">
  <ul class="navbar-nav">
    <li><a href="/"><%= image_tag image_path('IconE.png') %></a></li>
    <li><a href="/artists"><%= image_tag image_path('IconF.png') %></a></li>
    <li><a href="/contact"><%= image_tag image_path('IconB.png') %></a></li>
    <li><a href="/store"><%= image_tag image_path('IconC.png') %></a></li>
  </ul>
</nav>

<div class="logo"></div>

<div id="body-container">
  <%= yield %>
</div>

<%= render 'layouts/radio' %>

</body>
</html>
